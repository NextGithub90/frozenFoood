# Security: Protect sensitive files
# Nada Agen Sosis - .htaccess

# Deny access to config file
<Files "config.php">
    Order Allow,Deny
    Deny from all
</Files>

# Deny access to SQL files
<FilesMatch "\.(sql)$">
    Order Allow,Deny
    Deny from all
</FilesMatch>

# Prevent directory listing
Options -Indexes

# Set default charset
AddDefaultCharset UTF-8
